      "parameters": {
        "operation": "executeQuery",
        "query": "=INSERT INTO dossiers (\n  request_id,\n  customer_email,\n  letter_text,\n  status,\n  document_type,\n  victime_nom,\n  accident_date,\n  validated_fields\n) VALUES (\n  '{{ $json.request_id }}',\n  '{{ $json.customer_email }}',\n  '{{ $json.letter_text.replace(/'/g, \"''\") }}',\n  '{{ $json.status }}',\n  '{{ $json.document_type }}',\n  {{ $json.victime_nom ? \"'\" + $json.victime_nom.replace(/'/g, \"''\") + \"'\" : \"NULL\" }},\n  {{ $json.accident_date ? \"'\" + $json.accident_date + \"'\" : \"NULL\" }},\n  '{{ $json.validated_fields }}'::jsonb\n)\nON CONFLICT (request_id) DO UPDATE SET\n  letter_text = EXCLUDED.letter_text,\n  status = EXCLUDED.status,\n  validated_fields = EXCLUDED.validated_fields,\n  updated_at = NOW()\nRETURNING id, request_id",
