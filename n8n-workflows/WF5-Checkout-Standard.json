      "parameters": {
        "jsCode": "// Validate & Prepare checkout data\nconst body = $input.first().json.body || $input.first().json;\n\n// Validation\nif (!body.customer_email) {\n  return {\n    valid: false,\n    error: 'Email requis'\n  };\n}\n\nif (!body.letter_text) {\n  return {\n    valid: false,\n    error: 'Lettre requise'\n  };\n}\n\nconst requestId = body.request_id || 'req_' + Date.now();\nconst context = body.context || {};\n\n// Préparer les données\nreturn {\n  valid: true,\n  request_id: requestId,\n  customer_email: body.customer_email,\n  letter_text: body.letter_text,\n  status: 'pending_payment',\n  document_type: 'standard',\n  victime_nom: context.victime_nom || null,\n  accident_date: context.accident_date || null,\n  validated_fields: JSON.stringify({\n    victime: {\n      nom: context.victime_nom || '',\n      prenom: context.victime_prenom || ''\n    },\n    accident: {\n      date: context.accident_date || '',\n      lieu: context.accident_lieu || ''\n    },\n    employeur: {\n      nom_raison_sociale: context.employeur_nom || ''\n    }\n  })\n};"
