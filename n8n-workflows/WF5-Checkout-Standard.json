        "url": "https://api.stripe.com/v1/checkout/sessions",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "string",
        "bodyString": "=mode=payment&success_url={{ encodeURIComponent('https://accidentdoc.fr/succes?session_id={CHECKOUT_SESSION_ID}&rid=' + $json.request_id) }}&cancel_url={{ encodeURIComponent('https://accidentdoc.fr/paiement?cancelled=true&rid=' + $json.request_id) }}&customer_email={{ encodeURIComponent($json.customer_email) }}&line_items[0][price_data][currency]=eur&line_items[0][price_data][product_data][name]={{ encodeURIComponent('Lettre de réserves AT/MP') }}&line_items[0][price_data][unit_amount]=8280&line_items[0][quantity]=1&metadata[type]=standard&metadata[request_id]={{ $json.request_id }}&metadata[customer_email]={{ encodeURIComponent($json.customer_email) }}&metadata[victime_nom]={{ encodeURIComponent($json.stripe_metadata.victime_nom) }}&metadata[accident_date]={{ encodeURIComponent($json.stripe_metadata.accident_date) }}",
        "options": {
